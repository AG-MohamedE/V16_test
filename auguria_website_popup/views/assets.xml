<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="sh_inherit_assets_frontend_for_website_popup" inherit_id="website.assets_frontend" name="sh website popup">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/auguria_website_popup/static/src/scss/style.scss" />
        </xpath>
        <xpath expr="." position="inside">
            
            
            <script type="text/javascript" src="/auguria_website_popup/static/src/js/popup.js"></script>
            
            
            <script type="text/javascript" src="/auguria_website_popup/static/src/js/jquery_cookie.js"></script>
        </xpath>
    </template>

</odoo>